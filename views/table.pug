extends layout

block content
    div 
        a(href="/admin/csv") Download csv
    table  
        thead
            tr: th Id
                th Date
                th Name
                th Email
                th SSN
                th Count
        tbody 
            each row in list 
                tr 
                    td= row['id']
                    td= row['date']
                    td= row['name']
                    td= row['email']
                    td= row['ssn']
                    td= row['count']